{
  "name": "tiger-programming-language",
  "displayName": "Tiger Programming Language",
  "description": "Tiger Programming Language is a coding language developed to help build web applications, designed for the tiger App Library",
  "version": "1.1.0",
  "engines": {
    "vscode": "^1.69.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "tiger",
      "aliases": ["Tiger", "tiger"],
      "extensions": [".tgr"],
      "configuration": "./language-configuration.json",
      "icon": {
        "light": "./icon.png",
        "dark": "./icon.png"
      }
    }],
    "grammars": [{
      "language": "tiger",
      "scopeName": "source.tgr",
      "path": "./syntaxes/tiger.tmLanguage.json"
    }]
  },
  "keywords": [
		"tiger",
    ".tgr"
	],
  "repository": {
    "type": "git",
    "url": "https://github.com/aaron-meche/language.tgr.git"
  },
  "publisher": "aaron-meche"
}
